{
  "name": "react-starter",
  "version": "1.0.0",
  "homepage": "https://nikitastryuk.github.io/React-starter/",
  "scripts": {
    "dev": "NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "cypress": "cypress open",
    "test": "jest",
    "prettier": "prettier . --check",
    "lint:js": "eslint src",
    "lint:style": "stylelint src --config .stylelintrc",
    "generate-webpack-stats": "NODE_ENV=production webpack --config webpack/webpack.stats.js",
    "serve-prod-build": "npx serve -s -l 8080 -C -d build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "Nikita Striuk",
  "license": "ISC",
  "dependencies": {
    "@styled-icons/boxicons-logos": "10.23.0",
    "axios": "0.21.1",
    "i18next": "20.1.0",
    "i18next-browser-languagedetector": "6.1.0",
    "i18next-intervalplural-postprocessor": "3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.1",
    "react-i18next": "11.8.12",
    "react-query": "3.13.0",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1",
    "styled-theming": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.12",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.13.13",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "13.0.16",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-react-remove-properties": "0.3.0",
    "babel-plugin-styled-components": "1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "3.0.0",
    "cypress": "6.8.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-testing-library": "3.10.2",
    "eslint-webpack-plugin": "2.5.3",
    "gh-pages": "3.1.0",
    "html-webpack-plugin": "5.3.1",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "simple-git-hooks": "2.2.0",
    "styled-reset": "4.3.4",
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "21.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "webpack": "5.28.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  },
  "lint-staged": {
    "*": "npm run prettier",
    "*.js": [
      "npm run lint:js",
      "npm run lint:style"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "lint-staged"
  }
}
