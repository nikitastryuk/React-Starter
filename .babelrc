{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-syntax-dynamic-import",
    [
      "babel-plugin-styled-components",
      {
        "ssr": false
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "components": "./src/components",
          "app": "./src/app",
          "pages": "./src/pages",
          "i18n": "./src/i18n",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
